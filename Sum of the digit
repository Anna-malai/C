/*
 To find sum of the digit
  Input:
    1234
  Output:
     10
*/#include <stdio.h>
int main()
{
int num, sum = 0;
scanf("%d", &num); /* The number is entered from test case in num */
int temp;
temp=num;
while(num!=0){
 sum=sum+(num%10);
  num=num/10;
}


printf("Sum of the digits %d = %d", temp, sum);

}
