/*
  Perfect number or not
  //if it is prime number print YES or otherwise print NO
  Input:
   6 
  OUTPUT:
  YES
   
*/
#include <stdio.h>
int main()
{
  int n,sum; /*n to store the number, sum - to store the sum of factors*/

  scanf("%d",&n); /*Input the  number from the test case 6, 28 & 45 respectively */
   int i;
sum=0;
for(i=1;i<n;i++)
{
 if(n%i==0)
   sum=sum+i;
  }
 if(sum==n)
      printf("YES");
    else
      printf("NO");
}
